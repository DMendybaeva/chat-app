{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier", "plugin:react/jsx-runtime"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/no-extraneous-dependencies": [0],
    "react/prop-types": [0],
    "react/function-component-definition": [2, { "namedComponents": "arrow-function" }]
  }
}
